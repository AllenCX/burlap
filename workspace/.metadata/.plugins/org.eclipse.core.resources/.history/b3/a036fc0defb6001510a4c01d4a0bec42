package ellisPaperCode.taxi;

import java.util.List;

import burlap.behavior.singleagent.auxiliary.StateReachability;
import burlap.behavior.statehashing.DiscreteStateHashFactory;
import burlap.oomdp.auxiliary.StateGenerator;
import burlap.oomdp.core.Domain;
import burlap.oomdp.core.State;


public class TaxiRandomStateGenerator implements StateGenerator {
	private Domain d;
	private List<State> allStates;
	
	public TaxiRandomStateGenerator(Domain d, State initialState) {
		this.d = d;
		this.allStates = StateReachability.getReachableStates(initialState, (SADomain) d, new DiscreteStateHashFactory());

		
	}
	

	@Override
	public State generateState() {
		return null;
	}

}
